<div class="container">
  <h1>Add a new contact</h1>
  <form action="/contacts" method="post">
    <label for="new-name">Contact Name</label>
    <input id="new-name" name="new-name" type="text">
    <label for="new-phone">Phone Number</label>
    <input id="new-phone" name="new-phone" type="text">
    <label for="new-address">Street Address</label>
    <input id="new-address" name="new-address" type="text">
    <label for="new-email">Email Address</label>
    <input id="new-email" name="new-email" type="text">
    <button type="submit">Submit</button>
  </form>

  <a href="/">Go Back</a>
  <a href="/contacts">View all contacts</a>
</div>
